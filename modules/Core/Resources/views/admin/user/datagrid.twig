<table class="table table-striped table-bordered">
    <thead>
    <tr>
        <th>ID</th>
        <th>Email</th>
        <th>Created</th>
        <th>Updated</th>
    </tr>
    </thead>
    <tbody>
    {% for user in users %}
        <tr>
            <td>{{ user.id }}</td>
            <td>{{ user.email }}</td>
            <td>{{ user.updatedAt }}</td>
            <td>{{ user.createdAt }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>