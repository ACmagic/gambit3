{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/oddz/dbal"
        },
        {
            "type": "vcs",
            "url": "https://github.com/pingpong-labs/support"
        },
        {
            "type": "vcs",
            "url": "https://github.com/oddz/generators"
        },
        {
            "type": "vcs",
            "url": "https://github.com/oddz/modules"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "doctrine/dbal": "dev-dev-json-support as 2.5",
        "laravel/framework": "5.3.*",
        "laravel-doctrine/orm": "1.2.*",
        "pingpong/support": "dev-master",
        "pingpong/generators": "dev-dev-53-compatibility",
        "pingpong/modules": "dev-dev-53-compatibility",
        "laravel-doctrine/fluent": "1.1.*",
        "laravel-doctrine/migrations": "1.0.*",
        "laravel-doctrine/extensions": "1.0.*",
        "gedmo/doctrine-extensions": "^2.4",
        "beberlei/DoctrineExtensions": "^1.0",
        "rcrowe/twigbridge": "^0.9.1",
        "barryvdh/laravel-debugbar": "^2.2",
        "mesour/datagrid": "3.0.*@beta",
        "mesour/sources": "3.0.*@beta",
        "mesour/components": "3.0.*@beta",
        "mesour/table": "3.0.*@beta",
        "mesour/icon": "3.0.*@beta",
        "mesour/button": "3.0.*@beta",
        "mesour/pager": "3.0.*@beta",
        "mesour/dropdown": "3.0.*@beta",
        "mesour/selection": "3.0.*@beta",
        "mesour/filter": "3.0.*@beta",
        "mesour/laravel-bridges": "3.0.*@beta",
        "predis/predis": "^1.0",
        "nelmio/alice": "^2.1",
        "guzzlehttp/guzzle": "~6.0",
        "kris/laravel-form-builder": "1.6.*",
        "php-http/guzzle6-adapter": "^1.0",
        "payum/payum-laravel-package": "^1.0",
        "payum/payum": "^1.3",
        "ocramius/proxy-manager": "~2.0.2",
        "syslogic/doctrine-json-functions": "1.*@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
